{
  "version": 2,
  "builds": [
    {
      "src": "backend/app.js", // Adjust this if your server entry point is different
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "backend/frontend/build/index.html"
    }
  ],

}
