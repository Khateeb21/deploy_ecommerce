{
    "builds": [
      {
        "src": "frontend/build/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/app.js"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/build/$1"
      }
    ]
  }
  